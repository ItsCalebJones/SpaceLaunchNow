<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/launch_summary"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:tag="launch_summary"
    android:textAlignment="center"
    android:transitionGroup="true"
    tools:layout_behavior="@string/appbar_scrolling_view_behavior">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:layout_width="@dimen/max_width"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:layout_gravity="center"
            card_view:cardCornerRadius="@dimen/card_view_corner_radius"
            card_view:cardElevation="@dimen/material_card_elevation_resting"
            card_view:cardPreventCornerOverlap="true"
            card_view:cardUseCompatPadding="true">

            <LinearLayout
                android:id="@+id/mission_agencies"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/material_component_cards_primary_title_bottom_padding"
                    android:paddingTop="@dimen/material_component_cards_primary_title_top_padding">

                    <TextView
                        android:id="@+id/mission_agency_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:lines="1"
                        android:text="Launch Location Agencies"
                        android:textAlignment="center"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/material_typography_regular_title_text_size"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/mission_agency_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:lines="1"
                        android:singleLine="false"
                        android:text="Unknown Mission Agencies"
                        android:textAlignment="center"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_subheading_text_size" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/mission_one"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="@dimen/material_baseline_grid_0.5x"
                    android:weightSum="1">

                    <View

                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/View_Separator"
                        android:paddingLeft="12dp"
                        android:paddingRight="12dp" />

                    <TextView
                        android:id="@+id/mission_vehicle_agency_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingTop="@dimen/material_card_supporting_text_padding_top"
                        android:text="Agency One"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_subheading_text_size" />

                    <TextView
                        android:id="@+id/mission_agency_type_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="Unknown Agency Type"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_body_1_text_size" />

                    <TextView
                        android:id="@+id/mission_agency_summary_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="left"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:textColor="?android:textColorSecondary"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/material_baseline_grid_0.5x"
                        android:layout_weight="0.21">

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/mission_infoButton_one"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="INFO"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/mission_wikiButton_one"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="WIKI"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/mission_two"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <View

                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/View_Separator"
                        android:paddingLeft="12dp"
                        android:paddingRight="12dp" />

                    <TextView
                        android:id="@+id/mission_vehicle_agency_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingTop="@dimen/material_card_supporting_text_padding_top"
                        android:text="Unknown Agency"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_subheading_text_size" />

                    <TextView
                        android:id="@+id/mission_agency_type_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="Unknown Agency Type"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_body_1_text_size" />

                    <TextView
                        android:id="@+id/mission_agency_summary_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="left"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="@string/TsSKB_summary"
                        android:textColor="?android:textColorSecondary"
                        android:visibility="gone" />

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/material_baseline_grid_0.5x"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/mission_infoButton_two"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="INFO"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/mission_wikiButton_two"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="WIKI"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="@dimen/max_width"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:layout_gravity="center"
            card_view:cardCornerRadius="@dimen/card_view_corner_radius"
            card_view:cardElevation="@dimen/material_card_elevation_resting"
            card_view:cardPreventCornerOverlap="true"
            card_view:cardUseCompatPadding="true">

            <LinearLayout
                android:id="@+id/vehicle_agencies"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/material_component_cards_primary_title_bottom_padding"
                    android:paddingTop="@dimen/material_component_cards_primary_title_top_padding">

                    <TextView
                        android:id="@+id/launch_agency_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:lines="1"
                        android:text="Launch Location Agencies"
                        android:textAlignment="center"
                        android:textColor="?android:textColorPrimary"
                        android:textSize="@dimen/material_typography_regular_title_text_size"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/launch_agency_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:lines="1"
                        android:singleLine="false"
                        android:text="Unknown Mission Agencies"
                        android:textAlignment="center"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_subheading_text_size" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/agency_one"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="@dimen/material_baseline_grid_0.5x"
                    android:orientation="vertical">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_margin="8dp"
                        android:background="@color/View_Separator"
                        android:padding="12dp" />

                    <TextView
                        android:id="@+id/launch_vehicle_agency_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingTop="@dimen/material_card_supporting_text_padding_top"
                        android:text="Agency One"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_subheading_text_size" />

                    <TextView
                        android:id="@+id/launch_agency_type_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="Unknown Agency Type"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_body_1_text_size" />

                    <TextView
                        android:id="@+id/launch_agency_summary_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="left"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="@string/TsSKB_summary"
                        android:textColor="?android:textColorSecondary"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/material_baseline_grid_0.5x">

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/infoButton_one"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="INFO"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/wikiButton_one"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="WIKI"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/agency_two"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <View

                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_margin="16dp"
                        android:background="@color/View_Separator"
                        android:padding="12dp" />

                    <TextView
                        android:id="@+id/launch_vehicle_agency_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingTop="@dimen/material_card_supporting_text_padding_top"
                        android:text="Agency Two"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_subheading_text_size" />

                    <TextView
                        android:id="@+id/launch_agency_type_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:lines="1"
                        android:paddingBottom="@dimen/material_card_supporting_text_padding_bottom"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="Unknown Agency Type"
                        android:textColor="?android:textColorSecondary"
                        android:textSize="@dimen/material_typography_regular_body_1_text_size" />

                    <TextView
                        android:id="@+id/launch_agency_summary_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="left"
                        android:paddingLeft="@dimen/material_card_supporting_text_padding_horizontal"
                        android:paddingRight="@dimen/material_card_supporting_text_padding_horizontal"
                        android:text="@string/TsSKB_summary"
                        android:textColor="?android:textColorSecondary"
                        android:visibility="gone" />

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="@dimen/material_baseline_grid_0.5x">

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/infoButton_two"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="INFO"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/wikiButton_two"
                            style="@style/Base.Widget.AppCompat.Button.Colored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="WIKI"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>

</android.support.v4.widget.NestedScrollView>